config_version=5

[application]

config/name="Racer"
run/main_scene="res://scenes/title_screen.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Globals="*res://autoload/globals.gd"
SceneManager="*res://autoload/scene_manager.tscn"
SoundManager="*res://autoload/sound_manager.tscn"
SilentWolf="*res://addons/silent_wolf/SilentWolf.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=576
window/stretch/mode="viewport"

[editor]

movie_writer/movie_file="/home/antisune/Documents/Godot/Racer/video.avi"

[editor_plugins]

enabled=PackedStringArray("res://addons/silent_wolf/plugin.cfg")

[file_customization]

folder_colors={
"res://assets/": "purple",
"res://autoload/": "red",
"res://misc/": "yellow",
"res://misc/road/": "yellow",
"res://scenes/": "blue",
"res://ui/": "green",
"res://vehicle/": "yellow",
"res://vehicle/body/": "yellow",
"res://vehicle/data/": "gray",
"res://vehicle/npc/": "yellow",
"res://vehicle/player/": "blue"
}

[global_group]

no_transition_menu="A ui menu that should not have a trans animation"

[gui]

theme/custom="res://game_theme.tres"
theme/custom_font="res://assets/fonts/PublicPixel-z84yD.ttf"

